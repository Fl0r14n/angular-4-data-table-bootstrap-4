{
  "name": "ngx-datatable-bootstrap4",
  "version": "0.4.5",
  "description": "An Angular data table, with pagination, sorting, expandable rows etc.",
  "keywords": [
    "angular",
    "angular6",
    "Angular 6",
    "ng2",
    "datatable",
    "data-table",
    "data table",
    "Bootstrap",
    "Bootstrap 4",
    "pagination"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "tslint --force \"src/**/*.ts\"",
    "pretest": "npm run lint",
    "build": "npm run clean && npm run lint && ng-packagr -p ng-package.json",
    "watch": "webpack --config config/webpack.config.dev.ts --watch --progress --profile",
    "server": "webpack-dev-server --config config/webpack.config.dev.ts --inline --progress --port 8080",
    "start": "npm run server"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:cfasol/ngx-datatable-bootstrap4.git"
  },
  "peerDependencies": {
    "@angular/core": "*",
    "@angular/common": "*"
  },
  "author": "cfasol",
  "license": "MIT",
  "devDependencies": {
    "@angular/compiler": "*",
    "@angular/compiler-cli": "*",
    "@angular/platform-browser": "*",
    "@angular/platform-browser-dynamic": "*",
    "@types/angular": "*",
    "@types/core-js": "*",
    "@types/jasmine": "*",
    "@types/node": "10.1.4",
    "@types/rx": "*",
    "@types/webpack": "*",
    "angular2-template-loader": "*",
    "autoprefixer": "*",
    "bootstrap": "*",
    "codelyzer": "*",
    "copy-webpack-plugin": "*",
    "css-loader": "*",
    "extract-text-webpack-plugin": "next",
    "file-loader": "*",
    "font-awesome": "*",
    "fork-ts-checker-webpack-plugin": "*",
    "happypack": "next",
    "html-webpack-plugin": "*",
    "ng-packagr": "*",
    "node-sass": "*",
    "null-loader": "*",
    "postcss-loader": "*",
    "raw-loader": "*",
    "reflect-metadata": "*",
    "rimraf": "*",
    "rxjs": "*",
    "rxjs-compat": "*",
    "sass-loader": "*",
    "source-map-loader": "*",
    "style-loader": "*",
    "ts-helpers": "*",
    "ts-loader": "*",
    "ts-node": "*",
    "tslib": "*",
    "tslint": "*",
    "typescript": "2.7.2",
    "webpack": "*",
    "webpack-cli": "*",
    "webpack-dev-server": "^3.1.4",
    "zone.js": "*"
  },
  "dependencies": {
    "@angular/common": "*",
    "@angular/core": "*",
    "@angular/forms": "*"
  },
  "homepage": "https://github.com/cfasol/ngx-datatable-bootstrap4"
